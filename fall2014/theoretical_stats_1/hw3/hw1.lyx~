[2.11]
\end_layout

\end_inset


\begin_inset Formula $X\sim\frac{1}{\sqrt{2\pi}}e^{-x^{2}/2}$
\end_inset


\end_layout

\begin_deeper
\begin_layout Enumerate
\begin_inset ERT
status open

\begin_layout Plain Layout

\end_layout

\end_inset


\begin_inset Formula 
\begin{align*}
E\left(X^{2}\right) & =\frac{1}{\sqrt{2\pi}}\int_{-\infty}^{\infty}x^{2}e^{-x^{2}/2}dx\\
 & =\frac{1}{\sqrt{2\pi}}\int_{-\infty}^{\infty}x\cdot x\cdot e^{-x^{2}/2}dx\\
 & =\frac{1}{\sqrt{2\pi}}\left(\left.\left(x\int x\cdot e^{-x^{2}/2}dx\right)\right|_{-\infty}^{\infty}-\int_{-\infty}^{\infty}\left(\int x\cdot e^{-y^{2}/2}dy\right)dx\right)\\
 & =\frac{1}{\sqrt{2\pi}}\left(\left.x\left(-e^{-x^{2}/2}\right)\right|_{-\infty}^{\infty}+\int_{-\infty}^{\infty}e^{-x^{2}/2}dx\right)\\
 & =\frac{1}{\sqrt{2\pi}}\left(0+\int_{-\infty}^{\infty}e^{-x^{2}/2}dx\right)\\
 & =1
\end{align*}

\end_inset

By example 2.1.7 
\begin_inset Formula 
\[
f_{Y}(y)=\frac{1}{2\sqrt{2\pi y}}\left(e^{-y/2}+e^{-y/2}\right)=\frac{1}{\sqrt{2\pi y}}e^{-y/2}
\]

\end_inset

and since 
\begin_inset Formula $0<y<\infty$
\end_inset

 
\begin_inset Formula 
\begin{align*}
E\left(Y\right) & =\int_{0}^{\infty}yf_{Y}(y)dy\\
 & =\frac{1}{\sqrt{2\pi}}\int_{0}^{\infty}\frac{e^{-y/2}}{\sqrt{y}}dy\\
 & =\frac{1}{\sqrt{2\pi}}\frac{1}{2}\int_{-\infty}^{\infty}2e^{-\left(\sqrt{y}\right)^{2}/2}d\left(\sqrt{y}\right)\\
 & =\frac{1}{\sqrt{2\pi}}\int_{-\infty}^{\infty}e^{-\left(\sqrt{y}\right)^{2}/2}d\left(\sqrt{y}\right)\\
 & =\frac{1}{\sqrt{2\pi}}\sqrt{2\pi}\\
 & =1
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
The support of 
\begin_inset Formula $Y$
\end_inset

 is 
\begin_inset Formula $0<y<\infty$
\end_inset

.
 If 
\begin_inset Formula $-\infty<x<0$
\end_inset

 then 
\begin_inset Formula $Y=-X$
\end_inset

 and 
\begin_inset Formula $g(Y)^{-1}=-Y$
\end_inset

, else if 
\begin_inset Formula $0\leq x<\infty$
\end_inset

 then 
\begin_inset Formula $Y=X$
\end_inset

 and 
\begin_inset Formula $g(Y)^{-1}=Y$
\end_inset

.
 Then 
\begin_inset Formula 
\[
f_{Y}(y)=\frac{1}{\sqrt{2\pi}}\left(e^{-(-y)^{2}/2}\left|-1\right|+e^{-y^{2}/2}\left|1\right|\right)=\frac{2e^{-y^{2}/2}}{\sqrt{2\pi}}
\]

\end_inset

and therefore 
\begin_inset Formula 
\begin{align*}
E(Y) & =\frac{2}{\sqrt{2\pi}}\int_{0}^{\infty}ye^{-y^{2}/2}dy\\
 & =\frac{2}{\sqrt{2\pi}}\int_{0}^{\infty}e^{-(y^{2}/2)}d\left(y^{2}/2\right)\\
 & =\frac{2}{\sqrt{2\pi}}
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $\text{Var}(Y)=E\left(Y^{2}\right)-\left(E\left(Y\right)\right)^{2}$
\end_inset


\begin_inset Formula 
\begin{align*}
E\left(Y^{2}\right) & =\frac{2}{\sqrt{2\pi}}\int_{0}^{\infty}y^{2}e^{-y^{2}/2}dy\\
 & =2\frac{1}{2}\int_{-\infty}^{\infty}y^{2}\frac{e^{-y^{2}/2}}{\sqrt{2\pi}}dy\\
 & =1\text{ by part (a)}
\end{align*}

\end_inset

Hence 
\begin_inset Formula $\text{Var}\left(Y\right)=1-\frac{2}{\pi}$
\end_inset

.
\end_layout

\end_deeper
\begin_layout Enumerate
\begin_inset ERT
status open

\begin_layout Plain Layout

[2.12]
\end_layout

\end_inset


\begin_inset Formula $Y=g(X)=d\tan(X)$
\end_inset

.
 
\begin_inset Formula $g(X)$
\end_inset

 is increasing for 
\begin_inset Formula $0<x<\pi/2$
\end_inset

 and 
\begin_inset Formula $g^{-1}(Y)=\arctan(Y).$
\end_inset

 Hence
\begin_inset Formula 
\[
\left|\left(\arctan(Y/d)\right)^{'}\right|=\frac{1}{1+(Y/d)^{2}}\frac{1}{d}
\]

\end_inset

and therefore
\begin_inset Formula 
\[
f_{Y}(y)=\frac{2}{\pi}\frac{1}{1+(y/d)^{2}}\frac{1}{d}
\]

\end_inset

with support 
\begin_inset Formula $y\in(0,\infty).$
\end_inset

 This is the Cauchy distribution hence 
\begin_inset Formula $E(Y)=\infty$
\end_inset

.
\end_layout

\begin_layout Enumerate
\begin_inset ERT
status open

\begin_layout Plain Layout

[2.13]
\end_layout

\end_inset

The probability that there are 
\begin_inset Formula $k$
\end_inset

 heads, given the first flip lands heads, is geometrically distributed 
\begin_inset Quotes eld
\end_inset

flips until first tail
\begin_inset Quotes erd
\end_inset

: 
\begin_inset Formula $P_{H}(X=k)=p^{k}(1-p)$
\end_inset

 but restricted to 
\begin_inset Formula $k=1,2,3,\dots$
\end_inset

 and the probability that there are 
\begin_inset Formula $k$
\end_inset

 tails, given the first flip lands tails, is also geometrically distributed
 
\begin_inset Quotes eld
\end_inset

flips until first head
\begin_inset Quotes erd
\end_inset

: 
\begin_inset Formula $P_{T}(X=k)=(1-p)^{k}p$
\end_inset

, but also restricted to 
\begin_inset Formula $k=1,2,3,\dots$
\end_inset

.
 Therefore the probability that there's either a run of 
\begin_inset Formula $k$
\end_inset

 heads or tails is 
\begin_inset Formula 
\[
P_{H\vee T}(X=k)=P_{H}+P_{T}=p^{k}(1-p)+(1-p)^{k}p
\]

\end_inset

and 
\begin_inset Formula 
\begin{align*}
E\left(X\right) & =\sum_{k=1}^{\infty}k\left(p^{k}(1-p)+(1-p)^{k}p\right)\\
 & =\sum_{k=1}^{\infty}kp^{k}(1-p)+\sum_{k=1}^{\infty}k(1-p)^{k}p\\
 & =E\left(H\right)-(1-p)+E\left(T\right)-p\\
 & =\frac{1-(1-p)}{1-p}+\frac{1-p}{p}\\
 & =\frac{1}{p}+\frac{1}{1-p}-2
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset ERT
status open

\begin_layout Plain Layout

[2.14]
\end_layout

\end_inset


\end_layout

\begin_deeper
\begin_layout Enumerate
