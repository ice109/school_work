\begin_layout Plain Layout

[2.24]
\end_layout

\end_inset


\end_layout

\begin_deeper
\begin_layout Enumerate
\begin_inset Formula $E\left(X\right)=a\int_{0}^{1}x^{a}dx=\frac{a}{a+1}$
\end_inset

 and 
\begin_inset Formula $E\left(X^{2}\right)=a\int_{0}^{1}x^{a+1}dx=\frac{a}{a+2}$
\end_inset

 therefore 
\begin_inset Formula $\text{Var}(X)=E\left(X^{2}\right)-\left(E(X)\right)^{2}=\frac{a}{a+2}-\frac{a^{2}}{a^{2}+2a+1}=\frac{a}{(a^{2}+1)(a+2)}$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $E\left(X\right)=\frac{1}{n}\sum_{k=1}^{n}k=\frac{1}{n}\frac{n(n+1)}{2}=\frac{1}{2}(n+1)$
\end_inset

 and 
\begin_inset Formula $E\left(X^{2}\right)=\frac{1}{n}\sum_{k=1}^{n}k^{2}$
\end_inset

 but 
\begin_inset Formula 
\[
k^{3}-\left(k-1\right)^{3}=k^{3}-\left(k^{3}-3k^{2}+3k+1\right)=3k^{2}-3k-1
\]

\end_inset

and therefore
\begin_inset Formula 
\begin{align*}
\frac{1}{n}\sum_{k=1}^{n}k^{2} & =\frac{1}{3n}\sum_{k=1}^{n}k^{3}-\left(k-1\right)^{3}+3k-1\\
 & =\frac{1}{3n}\left(\sum_{k=1}^{n}\left(k^{3}-\left(k-1\right)^{3}\right)+\sum_{k=1}^{n}3k-1\right)\\
 & =\frac{1}{3n}\left(\sum_{k=1}^{n}\left(k^{3}-\left(k-1\right)^{3}\right)+\frac{3}{2}n(n+1)-n\right)\\
 & =\frac{1}{3n}\left(n^{3}+\frac{3}{2}n(n+1)-n\right)\\
 & =\frac{1}{6}(2n+1)(n+1)
\end{align*}

\end_inset

Hence 
\begin_inset Formula 
\begin{align*}
\text{Var}(X) & =\frac{1}{6}(2n+1)(n+1)-\frac{1}{4}(n+1)(n+1)\\
 & =\frac{1}{2}(n+1)\left(\frac{1}{3}(2n+1)-\frac{1}{2}(n+1)\right)\\
 & =\frac{1}{12}(n+1)\left(n-1\right)\\
 & =\frac{n^{2}-1}{12}
\end{align*}

\end_inset


\end_layout

