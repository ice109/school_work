\[
f_{X}\left(x|\mu,\sigma\right)=\frac{1}{\sigma}e^{-\frac{x-\mu}{\sigma}}I\left(x\right)_{\left[\mu,\infty\right)}\text{ for }\mu,\sigma>0
\]

\end_inset

then 
\begin_inset Formula 
\[
f_{\mathbf{X}}\left(\mathbf{x}|\mu,\sigma\right)=\left(\frac{1}{\sigma}e^{\frac{\mu}{\sigma}}\right)^{n}e^{-\frac{\sum_{i=1}^{n}x_{i}}{\sigma}}\prod_{i=1}^{n}I\left(x_{i}\right)_{\left[\mu,\infty\right)}\text{ for }\mu,\sigma>0
\]

\end_inset

but 
\begin_inset Formula $\mathbf{X}\in\prod_{i=1}^{n}\left[\mu,\infty\right)\iff\forall i\; X_{i}\geq\mu\iff\min_{i}\left(X_{i}\right)\geq\mu$
\end_inset

 so
\begin_inset Formula 
\begin{align*}
f_{\mathbf{X}}\left(\mathbf{x}|\mu,\sigma\right) & =\left(\frac{1}{\sigma}e^{\frac{\mu}{\sigma}}\right)^{n}e^{-\frac{\sum_{i=1}^{n}x_{i}}{\sigma}}I\left(\min_{i}\left(x_{i}\right)\right)_{\left[\mu,\infty\right)}\text{ for }\mu,\sigma>0
\end{align*}

\end_inset

Therefore 
\begin_inset Formula $\left(\bar{X},\min_{i}\left(X_{i}\right)\right)$
\end_inset

 is a sufficient statistic.
