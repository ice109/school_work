\end_inset

.
\end_layout

\begin_layout Enumerate
If 
\begin_inset Formula 
\[
f_{X}\left(x|\theta\right)=e^{-\left(x-\theta\right)}e^{-e^{-\left(x-\theta\right)}}I\left(x\right)_{\mathbb{R}}\text{ for }\theta\in\mathbb{R}
\]

\end_inset

then
\begin_inset Formula 
\begin{align*}
f_{\mathbf{X}}\left(\mathbf{x}|\theta\right) & =e^{-\sum_{i=1}^{n}\left(x_{i}-\theta\right)}e^{-\sum_{i=1}^{n}e^{-\left(x_{i}-\theta\right)}}I\left(x\right)_{\mathbb{R}^{n}}\text{ for }\theta\in\mathbb{R}\\
 & =e^{n\theta}e^{-\sum_{i=1}^{n}x_{i}}e^{-e^{\theta}\sum_{i=1}^{n}e^{-x_{i}}}I\left(x\right)_{\mathbb{R}^{n}}\text{ for }\theta\in\mathbb{R}\\
 & =e^{n\theta}e^{-\sum_{i=1}^{n}x_{i}}\left(e^{-\sum_{i=1}^{n}e^{-x_{i}}}\right)^{e^{\theta}}I\left(x\right)_{\mathbb{R}^{n}}\text{ for }\theta\in\mathbb{R}
\end{align*}

\end_inset

and for distinct, but otherwise arbitrary 
\begin_inset Formula $\mathbf{x}$
\end_inset

 and 
\begin_inset Formula $\mathbf{y}$
\end_inset

 
\begin_inset Formula 
\begin{align*}
\frac{f_{\mathbf{X}}\left(\mathbf{x}|\theta\right)}{f_{\mathbf{X}}\left(\mathbf{y}|\theta\right)} & =\frac{e^{n\theta}e^{-\sum_{i=1}^{n}x_{i}}\left(e^{-\sum_{i=1}^{n}e^{-x_{i}}}\right)^{e^{\theta}}I\left(x\right)_{\mathbb{R}^{n}}}{e^{n\theta}e^{-\sum_{i=1}^{n}y_{i}}\left(e^{-\sum_{i=1}^{n}e^{-y_{i}}}\right)^{e^{\theta}}I\left(x\right)_{\mathbb{R}^{n}}}\text{ for }\theta\in\mathbb{R}\\
 & =\frac{e^{-\sum_{i=1}^{n}x_{i}}\left(e^{-\sum_{i=1}^{n}e^{-x_{i}}}\right)^{e^{\theta}}}{e^{-\sum_{i=1}^{n}y_{i}}\left(e^{-\sum_{i=1}^{n}e^{-y_{i}}}\right)^{e^{\theta}}}\text{ for }\theta\in\mathbb{R}
\end{align*}

\end_inset

This is independent of 
\begin_inset Formula $\theta$
\end_inset

 iff 
\begin_inset Formula $\sum_{i=1}^{n}e^{-y_{i}}=\sum_{i=1}^{n}e^{-x_{i}}$
\end_inset

 which is the case iff 
\begin_inset Formula $\left(x_{\left(1\right)},\dots,x_{\left(n\right)}\right)=\left(y_{\left(1\right)},\dots,y_{\left(n\right)}\right)$
\end_inset

 and hence the order statistics 
\begin_inset Formula $T\left(\mathbf{X}\right)=\left(X_{\left(1\right)},\dots,X_{\left(n\right)}\right)$
\end_inset

 is minimally sufficient.
 Yet 
\begin_inset Formula $f_{X}$
\end_inset

 is a location parameter family with location parameter 
\begin_inset Formula $\theta$
\end_inset

 and so by Ex.
 6.2.18 the range 
\begin_inset Formula $R\left(\mathbf{X}\right)=X_{\left(n\right)}-X_{\left(1\right)}$
\end_inset

 is ancillary.
\end_layout

\begin_layout Enumerate
It's the case that 
\begin_inset Formula 
\begin{align*}
f_{X}\left(x|\theta\right) & =\binom{2}{x}\theta^{x}\left(1-\theta\right)^{2-x}I\left(x\right)_{\left\{ 0,1,2\right\} }\text{ for }\theta\in\left[0,1\right]\\
 & =\binom{2}{x}I\left(x\right)_{\left\{ 0,1,2\right\} }\left(1-\theta\right)^{2}\left(\frac{\theta}{1-\theta}\right)^{x}\text{ for }\theta\in\left[0,1\right]\\
 & =\binom{2}{x}I\left(x\right)_{\left\{ 0,1,2\right\} }\left(1-\theta\right)^{2}e^{x\log\left(\frac{\theta}{1-\theta}\right)}\text{ for }\theta\in\left[0,1\right]
\end{align*}

\end_inset

so 
\begin_inset Formula $X$
\end_inset

 is a random variable from an exponential family
\begin_inset Formula 
\[
f\left(x|\theta\right)=h\left(x\right)c\left(\theta\right)e\left(\sum_{j=1}^{k}w_{j}\left(\theta\right)t_{j}\left(x\right)\right)
\]

\end_inset

where 
\begin_inset Formula 
\begin{align*}
h\left(x\right) & =\binom{2}{x}I\left(x\right)_{\left\{ 0,1,2\right\} }\\
c\left(\theta\right) & =\left(1-\theta\right)^{2}\\
w_{1}\left(\theta\right) & =\log\left(\frac{\theta}{1-\theta}\right)\\
t_{1}\left(x\right) & =x
\end{align*}

\end_inset

and noting that 
\begin_inset Formula $w_{1}\left(y\right)=\log\left(\frac{y}{1-y}\right)$
\end_inset

 is continuous and invertible on 
\begin_inset Formula $\left(0,1\right)$
\end_inset

 by Thm 6.2.25 
\begin_inset Formula $T\left(\mathbf{X}\right)=\sum_{i=1}^{n}x_{i}$
\end_inset

 is complete for 
\begin_inset Formula $\theta$
\end_inset

.
\end_layout

